# Django Settings
DJANGO_ENV=development  # Options: development, staging, production
DJANGO_SECRET_KEY=your-secret-key-here
DJANGO_SETTINGS_MODULE=prashnakosh.settings_dev

# Database Settings (Supabase)
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password
DB_HOST=your_database_host
DB_PORT=5432

# Staging Database (if different)
STAGING_DB_NAME=your_staging_database_name

# Email Settings
EMAIL_HOST=smtp.your-email-provider.com
EMAIL_PORT=587
EMAIL_HOST_USER=your-email@example.com
EMAIL_HOST_PASSWORD=your-email-password
ADMIN_EMAIL=admin@prashnakosh.in

# Google Analytics
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Ollama Model Settings
OLLAMA_MODEL=llama3.2:1b

# Sentry Error Tracking (Optional)
SENTRY_DSN=https://your-sentry-dsn

# Other Settings
DEBUG=True  # Set to False in production 